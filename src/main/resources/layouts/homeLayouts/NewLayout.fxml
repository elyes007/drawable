                           translateY="5.0"/>
                <Button fx:id="export" alignment="TOP_CENTER" contentDisplay="BOTTOM" maxHeight="-Infinity"
                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        prefHeight="17.0" prefWidth="50.0" text="Export" textAlignment="CENTER" translateX="540.0"
                        translateY="5.0">
                    <font>
                        <Font size="8.0"/>
                    </font>
                </Button>
