<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="tn.disguisedtoast.drawable.settingsModule.controllers.menuSettings.MenuSettingsController">
    <top>
        <BorderPane minWidth="780.0" BorderPane.alignment="CENTER">
            <left>
                <Label styleClass="screen_message" text="Menu Settings" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
            </left>
            <center>
                <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                    <children>
                        <Pane prefHeight="0.5" styleClass="light_separator"/>
                    </children>
                    <BorderPane.margin>
                        <Insets left="10.0"/>
                    </BorderPane.margin>
                </VBox>
            </center>
            <BorderPane.margin>
                <Insets left="10.0"/>
            </BorderPane.margin>
        </BorderPane>
    </top>
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
    </padding>
    <center>
        <Button fx:id="addButtonButton" minWidth="200.0" mnemonicParsing="false" text="Add a button"
                BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="20.0"/>
            </BorderPane.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </center>
</BorderPane>
